<?xml version="1.0" encoding="UTF-8"?>
<routes id="routes-d40107f9-347e-465b-8449-d95a02e52b69" xmlns="http://camel.apache.org/schema/spring">
    <route id="SearchApplic_simple-route">
        <from id="SearchApplic_to2" uri="direct:mainsearchapplic"/>
        <to id="SearchApplic_systemTo1" uri="direct:_SystemVariables"/>
        <to id="SearchApplic_to1" uri="direct:SearchApplic_FilterVariables"/>
        <to id="SearchApplic_to3" uri="direct:SearchApplic_FilterEvaluation"/>
        <log id="SearchApplic_SQL" message="Select * from customers where ${exchangeProperty.SearchApplic_FilterConditions}"/>
        <setBody id="SearchApplicroute-setBody">
            <simple>Select * from customers where ${exchangeProperty.SearchApplic_FilterConditions}</simple>
        </setBody>
        <to id="SearchApplic_to5ddd" uri="jdbc:datasource?useHeadersAsParameters=true"/>
        <to id="SearchApplic_return" uri="direct:_Return"/>
    </route>
    <route id="SearchApplic_route2">
        <from id="SearchApplic_to2sssc" uri="direct:SearchApplic_FilterEvaluation"/>
        <log id="SearchApplic_log3" message="${exchangeProperty.UserID} != null"/>
        <choice id="SearchApplic_choice1">
            <when id="SearchApplic_when1">
                <simple>${exchangeProperty.UserID} != null</simple>
                <setProperty id="SearchApplic_setProperty1" propertyName="F_UserID">
                    <simple>AND UserID LIKE :?H_UserID</simple>
                </setProperty>
                <setHeader headerName="H_UserID" id="SearchApplicserachapplic_H_UserID">
                    <simple>%${exchangeProperty.UserID}%</simple>
                </setHeader>
            </when>
        </choice>
        <choice id="SearchApplic_choice2">
            <when id="SearchApplic_when2">
                <simple>  ${exchangeProperty.BranchCode} != null</simple>
                <setProperty id="SearchApplic_setProperty2" propertyName="F_BranchCode">
                    <simple>AND BranchCode LIKE :?H_BranchCode</simple>
                </setProperty>
                <setHeader headerName="H_BranchCode" id="SearchApplicserachapplic_F_BranchCode">
                    <simple>%${exchangeProperty.BranchCode}%</simple>
                </setHeader>
            </when>
        </choice>
        <choice id="SearchApplic_choice3">
            <when id="SearchApplic_when3">
                <simple>${exchangeProperty.CriteriaType} != null</simple>
                <setProperty id="SearchApplic_setProperty3" propertyName="F_CriteriaType">
                    <simple>AND CriteriaType LIKE :?F_CriteriaType</simple>
                </setProperty>
                <setHeader headerName="F_CriteriaType" id="SearchApplicserachapplic_F_CriteriaType">
                    <simple>%${exchangeProperty.CriteriaType}%</simple>
                </setHeader>
            </when>
        </choice>
        <choice id="SearchApplic_choice5">
            <when id="SearchApplic_when5">
                <simple>${exchangeProperty.CriteriaValue} != null</simple>
                <setProperty id="SearchApplic_setProperty5" propertyName="F_CriteriaValue">
                    <simple>AND CriteriaValue LIKE :?F_CriteriaValue</simple>
                </setProperty>
                <setHeader headerName="F_CriteriaValue" id="SearchApplicserachapplic_F_CriteriaValue">
                    <simple>%${exchangeProperty.CriteriaValue}%</simple>
                </setHeader>
            </when>
        </choice>
        <choice id="SearchApplic_choice14">
            <when id="SearchApplic_when14">
                <simple>${exchangeProperty.MerchantID} != null</simple>
                <setProperty id="SearchApplic_setProperty14" propertyName="F_MerchantID">
                    <simple>AND MerchantID LIKE '%${exchangeProperty.MerchantID}</simple>
                </setProperty>
                <setHeader headerName="F_MerchantID" id="SearchApplicserachapplic_F_MerchantID">
                    <simple>5${exchangeProperty.MerchantID}%</simple>
                </setHeader>
            </when>
        </choice>
        <setProperty id="SearchApplic_setProperty16" propertyName="SearchApplic_FilterConditions">
            <simple>1 = 1 ${exchangeProperty.F_UserID} ${exchangeProperty.F_BranchCode}  ${exchangeProperty.F_CriteriaValue}  ${exchangeProperty.F_CriteriaType}  ${exchangeProperty.F_MerchantID}  </simple>
        </setProperty>
        <log id="SearchApplic_log1" message="${exchangeProperty.SearchApplic_FilterConditions}"/>
    </route>
</routes>
